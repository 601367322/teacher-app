<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.prance.teacher.weight.PKRankBackground
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <TextView
        android:id="@+id/rankResult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <include
        android:id="@+id/rank2"
        layout="@layout/include_pk_rank"
        android:layout_width="@dimen/m260.0"
        android:layout_height="@dimen/m380.0"
        android:layout_marginStart="@dimen/m170.0"
        android:layout_marginTop="@dimen/m300.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/rank1"
        layout="@layout/include_pk_rank"
        android:layout_width="@dimen/m260.0"
        android:layout_height="@dimen/m380.0"
        android:layout_marginStart="@dimen/m565.0"
        android:layout_marginTop="@dimen/m260.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/rank3"
        layout="@layout/include_pk_rank"
        android:layout_width="@dimen/m260.0"
        android:layout_height="@dimen/m380.0"
        android:layout_marginStart="@dimen/m955.0"
        android:layout_marginTop="@dimen/m320.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/rank4"
        layout="@layout/include_pk_rank"
        android:layout_width="@dimen/m260.0"
        android:layout_height="@dimen/m380.0"
        android:layout_marginStart="@dimen/m1490.0"
        android:layout_marginTop="@dimen/m360.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <com.prance.teacher.weight.CountTimerFinish
        android:id="@+id/time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/m40.0"
        android:layout_marginEnd="@dimen/m40.0"
        android:letterSpacing="0.1"
        android:text="TextView"
        android:textColor="#FFFEFE"
        android:textSize="@dimen/m60.0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="#D86410"
        app:strokeWidth="@dimen/m5.0" />

</android.support.constraint.ConstraintLayout>